{
    "name": "Tab Ultra",
    "description": "organises tabs in groups",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },

    "icons": {
        "16": "Images/icon16.png",
        "32": "Images/icon32.png",
        "64": "Images/icon64.png",
        "128": "Images/icon128.png"
      },
    "permissions":[
    

        "tabs", "storage"


    ],
    "content_scripts":[
        {

            "matches":["<all_urls>"],
          
            "js": ["popup.js"]
            

        },
        {

            "matches":["<all_urls>"],
          
            "js": ["saved.js"]
        }


    ],

    "action":{

        "default_popup":"popup.html",

        "css":["style.css"]
    }

}